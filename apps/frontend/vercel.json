{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://zero-balance-production.up.railway.app/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
